<div id="feedID">
  <h3>Feed</h3>

  <% @offers.each do |offer| %>
    <div id="inside">
      <strong><%= offer.name %></strong><br />
      <%= offer.description %>
      <span id="offerDate" class="pull-right"><%= offer.created_at %></span><br />
      <%= button_to 'Favorite', line_items_path(offer_id: offer) %>
    </div>
  <% end %>
</div>
