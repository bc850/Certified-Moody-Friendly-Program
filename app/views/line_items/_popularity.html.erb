<div id="popularityDiv">
  <% @popularity.each do |offer| %>
    <strong><%= link_to offer.name, offer_path(offer.id) %></strong>
    <p id="popularityParagraph">
      <span id="redColorSpan"><%= offer.popularity %></span>
      favorites for this
      <%= offer.offering_type %>!</p>
  <% end %>
</div>
